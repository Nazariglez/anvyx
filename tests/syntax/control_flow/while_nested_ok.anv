// @mode: check
// @expect: success

fn main() {
    var i: int = 0;
    while i < 3 {
        var j: int = 0;
        while j < 3 {
            if j == 1 {
                j = j + 1;
                continue;
            }

            if j == 2 {
                break;
            }

            j = j + 1;
        }

        i = i + 1;
    }
}

